### Backend
APP_ENV=dev
APP_HTTP_ADDR=:8081
APP_BASE_URL=http://localhost:8081
APP_CONFIG_PATH=./docker/config.yaml

### Docker images
POSTGRES_IMAGE=postgres:16-alpine
SEARXNG_IMAGE=searxng/searxng:latest
PROXY_MUX_IMAGE=hightemp/go_proxy_mux:1.0.2

### Docker paths
POSTGRES_DATA_VOLUME=pg_data
SEARXNG_SETTINGS_PATH=./searxng/settings.yml
SEARXNG_SETTINGS_CONTAINER_PATH=/etc/searxng/settings.yml
PROXY_MUX_CONFIG_PATH_HOST=./proxy_mux/config.yaml
PROXY_MUX_CONFIG_PATH_CONTAINER=/root/config.yaml
BACKEND_BUILD_CONTEXT=../backend
BACKEND_DOCKERFILE=Dockerfile
BACKEND_ENV_FILE=./.env
BACKEND_CONFIG_PATH_HOST=./config.yaml
BACKEND_CONFIG_PATH_CONTAINER=/app/config.yaml
FRONTEND_BUILD_CONTEXT=../frontend
FRONTEND_DOCKERFILE=Dockerfile

### Docker ports
POSTGRES_PORT_HOST=5434
POSTGRES_PORT_CONTAINER=5432
SEARXNG_PORT_HOST=8083
SEARXNG_PORT_CONTAINER=8080
PROXY_MUX_PORT_HOST=8381
PROXY_MUX_PORT_CONTAINER=8380
BACKEND_PORT_HOST=8084
BACKEND_PORT_CONTAINER=8081
FRONTEND_PORT_HOST=3000
FRONTEND_PORT_CONTAINER=80

# Postgres
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_DB=gosearch
POSTGRES_USER=gosearch
POSTGRES_PASSWORD=gosearch
DATABASE_URL=postgres://gosearch:gosearch@postgres:5432/gosearch?sslmode=disable

### Auth
JWT_ISSUER=gosearch-ai
JWT_ACCESS_TTL=15m
JWT_REFRESH_TTL=720h
JWT_SIGNING_KEY=dev-only-change-me

### OpenRouter
OPENROUTER_API_KEY=
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1

### Pipeline
PIPELINE_TIMEOUT=120s
SEARCH_TIMEOUT=20s
FETCH_TIMEOUT=20s
OPENROUTER_TIMEOUT=60s
PAGE_CACHE_TTL=24h
SEARCH_MAX_QUERIES=3
SEARCH_MAX_SOURCES=5
SNIPPET_MAX_PER_SOURCE=3
CHAT_HISTORY_LIMIT=12

### SearxNG
SEARXNG_BASE_URL=http://searxng:8080

### Proxy (container runtime)
PROXY_HTTP=http://proxy_mux:8380
PROXY_HTTPS=http://proxy_mux:8380
PROXY_NO=localhost,127.0.0.1,proxy_mux,searxng,postgres,backend
